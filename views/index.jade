    script(src='/static/assets/js/classie.js')
    // Custom js
    script(type='text/javascript', src='/static/assets/js/custom.js')
    script.
      $('document').ready(function(){
        $('#NewsletterForm').on('submit',function(e){
          e.preventDefault()
          $.post( "/mailer",$( "#NewsletterForm" ).serialize(), function(data) {
            $( ".msgbox" ).toggleClass(('sucess')).html(data);
          }).fail(function(data) {
            $( ".msgbox" ).toggleClass(('error')).html(data.responseText);
          });
        })

        $('#ContactForm').on('submit',function(e){
          e.preventDefault()
          $.post( "/contact",$( "#ContactForm" ).serialize(), function(data) {
            $( ".msgboxcontact" ).toggleClass(('sucess')).html(data);
          }).fail(function(data) {
            $( ".msgboxcontact" ).toggleClass(('error')).html(data.responseText);
          })
        })
      })
